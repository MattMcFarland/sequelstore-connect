{
  "name": "sequelstore-connect",
  "version": "1.0.2",
  "description": "An express-session store for Sequelize.js",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm test && npm run build",
    "test": "npm run lint && npm run check && mocha $npm_package_options_mocha",
    "lint": "eslint src",
    "check": "flow check",
    "build": "rm -rf lib/* && babel src --ignore __tests__ --out-dir lib",
    "watch": "babel tasks/watch.js | node",
    "cover": "babel-node node_modules/.bin/isparta cover --root src --report html node_modules/.bin/_mocha -- $npm_package_options_mocha",
    "cover:lcov": "babel-node node_modules/.bin/isparta cover --root src --report lcovonly node_modules/.bin/_mocha -- $npm_package_options_mocha"
  },
  "options": {
    "mocha": "--require tasks/mocha-bootload src/**/__tests__/**/*.js"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-flow-strip-types": "^6.3.15",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "body-parser": "^1.14.2",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "compression": "^1.6.0",
    "cookie-parser": "^1.3.5",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-plugin-flow-vars": "^0.1.3",
    "express": "~4.13.1",
    "express-session": "^1.12.1",
    "figlet": "^1.1.1",
    "flow-bin": "^0.20.1",
    "isparta": "^4.0.0",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "sane": "^1.3.0",
    "sequelize": "^3.15.1",
    "sequelize-cli": "^2.2.1",
    "serve-favicon": "~2.3.0",
    "sqlite3": "^3.1.1",
    "supertest": "^1.1.0",
    "winston": "^2.1.1"
  },
  "keywords": [
    "session",
    "express-session",
    "express",
    "connect",
    "sequelize",
    "sql",
    "mssql",
    "mysql",
    "orm"
  ],
  "author": "Matt McFarland",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "mocha": "^2.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MattMcFarland/sequelstore-connect.git"
  },
  "bugs": {
    "url": "https://github.com/MattMcFarland/sequelstore-connect/issues"
  },
  "homepage": "https://github.com/MattMcFarland/sequelstore-connect#readme"
}
